{
  "testMatch": ["**/?(*)+(.test.js?(x))"],
  "testPathIgnorePatterns":["/node_modules/"],
  "collectCoverageFrom": [
    "*.test.js"
  ],
  "coverageReporters": [
    "html"
],
  "coverageDirectory": "./coverage",
  "collectCoverage":true,
   "moduleNameMapper": {
    "^.+\\.(css|scss)$": "babel-jest"
  }
}
